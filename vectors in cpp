
#include <iostream>
#include<array>
#include<vector>
using namespace std;

int main() {
    vector<int>v;
    vector<int> a(5,1);
    for(int i =0;i<a.size();i++){
        cout<<a[i];
    }
    
    
    cout<<endl;
    
    
    cout<<"size of this vector "<<v.capacity()<<endl;
    v.push_back(1);
    cout<<"size of this vector "<<v.capacity()<<endl;
    v.push_back(2);
    cout<<"size of this vector "<<v.capacity()<<endl;
    v.push_back(3);
    cout<<"size of this vector "<<v.capacity()<<endl;
    cout<<"size of this vector "<<v.size()<<endl;
    
    cout<<"element at 2nd index "<<v.at(2)<<endl;
    
    for(int i =0;i<v.size();i++){
        cout<<v[i]<<endl;
        
    }
    cout<<endl;
    cout<<endl;
    
    cout<<"front element in this vector  "<<v.front()<<endl;
    cout<<"last element in this vector  "<<v.back()<<endl;
    
    cout<<"before pop"<<endl;
    for(int i =0;i<v.size();i++){
        cout<<v[i]<<endl;
    }
    v.pop_back();
    
    cout<<"after pop"<<endl;
    for(int i =0;i<v.size();i++){
        cout<<v[i]<<endl;
    }
    
   cout<<" size before clear "<<v.size()<<endl;
   cout<<" capacity before clear" <<v.capacity()<<endl;
    v.clear();
    cout<<" size after clear "<<v.size()<<endl;
   cout<<" capacity after clear" <<v.capacity()<<endl;
   
   
    
    
}
